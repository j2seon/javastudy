package exer;

public class main {
	
	double computeAverage(int[] intArray) {
        double sum=0;
        for(int i=0; i<intArray.length; i++){
            sum+=intArray[i];
            return sum;
        }
        System.out.println(sum);
        return sum/intArray.length;
        
    }
	
	
	public static void main(String[] args) {

		main m = new main();
		
		System.out.println(m.computeAverage(new int[] {1,2,3,4,}));
		
		
	}

}
